{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Demo-n2d"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"GitHub_duan_n2d_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub_duan_n2d'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://demon2d.dfs.core.windows.net/"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0827-102253-5fcp0o42"
		},
		"AzureKeyVaultN2DLocalhost_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://KV-demo-n2d.vault.azure.net/"
		},
		"GitHub_duan_n2d_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "duan-n2d"
		},
		"OnPremSQLServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"OnPremSQLServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2019"
		},
		"OnPremSQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "n2d"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Demo-n2d_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"GitHub_duan_n2d_credential": {
						"value": "[parameters('GitHub_duan_n2d_credential')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVaultN2DLocalhost_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultN2DLocalhost_properties_typeProperties_baseUrl')]"
					},
					"GitHub_duan_n2d_properties_typeProperties_username": {
						"value": "[parameters('GitHub_duan_n2d_properties_typeProperties_username')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"OnPremSQLServer_properties_typeProperties_server": {
						"value": "[parameters('OnPremSQLServer_properties_typeProperties_server')]"
					},
					"OnPremSQLServer_properties_typeProperties_database": {
						"value": "[parameters('OnPremSQLServer_properties_typeProperties_database')]"
					},
					"OnPremSQLServer_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSQLServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}